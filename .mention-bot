{
  "maxReviewers": 3,
  "numFilesToCheck": 10,
  "message": "@reviewers, please review this.",
  "alwaysNotifyForPaths": [
    {
      "name": "toabctl",
      "files": [
        "{**/,}*+(cinder|manila)*{/**,}"
      ]
    },
    {
      "name": "rhafer",
      "files": [
        "{**/,}*+(keystone|neutron)*{/**,}"
      ]
    },
    {
      "name": "aspiers",
      "files": [
        "{**/,}*[^a-z]ha[^a-z]*{/**,}"
      ]
    }
  ],
  "fallbackNotifyForPaths": [
    {
      "name": "vuntz",
      "files": ["crowbar_framework/**"]
    }
  ],
  "findPotentialReviewers": true,
  "fileBlacklist": [],
  "userBlacklist": [],
  "userBlacklistForPR": [],
  "requiredOrgs": ["crowbar"],
  "actions": ["opened", "synchronize"],
  "skipAlreadyAssignedPR": true,
  "assignToReviewer": false,
  "skipTitle": "",
  "delayed": false,
  "delayedUntil": "3d",
  "skipCollaboratorPR": false
}
