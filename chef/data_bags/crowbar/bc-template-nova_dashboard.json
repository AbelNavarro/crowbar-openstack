{
  "id": "bc-template-nova_dashboard",
  "description": "User Interface for OpenStack projects (code name Horizon)",
  "attributes": {
    "nova_dashboard": {
      "sql_engine": "mysql",
      "keystone_instance": "none",
      "mysql_instance": "none",
      "gitrepo": "http://github.com/openstack/horizon.git",
      "git_refspec": "master",
      "use_gitbarclamp": true,
      "pfs_deps": [
        "apache2",
        "libapache2-mod-wsgi",
        "python-lockfile",
        "pip://warlock<2",
        "pip://django>=1.4",
        "pip://django_compressor",
        "pip://django_openstack_auth",
        "pip://python-cinderclient",
        "pip://python-glanceclient<2",
        "pip://python-keystoneclient",
        "pip://python-novaclient",
        "pip://python-quantumclient>=2.0",
        "pip://python-swiftclient>1.1,<1.2",
        "pip://pytz"
      ],
      "use_gitrepo": true
    }
  },
  "deployment": {
    "nova_dashboard": {
      "crowbar-revision": 0,
      "element_states": {
        "nova_dashboard-server": [ "readying", "ready", "applying" ]
      },
      "elements": {},
      "element_order": [
        [ "nova_dashboard-server" ]
      ],
      "config": {
        "environment": "nova_dashboard-base-config",
        "mode": "full",
        "transitions": false,
        "transition_list": [
        ]
      }
    }
  }
}

