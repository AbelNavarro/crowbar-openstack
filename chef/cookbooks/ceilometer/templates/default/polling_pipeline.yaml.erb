---
sources:
    - name: swift_source
      interval: <%= @swift_interval %>
      meters:
        - "storage.objects"
        - "storage.objects.size"
        - "storage.objects.containers"
      sinks:
        - meter_sink
sinks:
    - name: meter_sink
      transformers:
      publishers:
          - monasca://<%= @monasca_api_url %>
